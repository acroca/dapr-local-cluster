{
  "profiles": {
    "http": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5048"
    },
    "https": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7283;http://localhost:5048"
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "8081",
        "ASPNETCORE_HTTP_PORTS": "8080"
      },
      "publishAllPorts": true,
      "useSSL": true
    },
    "dapr": {
      "commandName": "Executable",
      "executablePath": "dapr.exe",
      "commandLineArgs": "run --app-id dsnext-api --app-port 7283 --app-protocol https -- dotnet run --project . --launch-profile https --urls https://localhost:7283",
      "workingDirectory": "$(ProjectDir)",
      "remoteDebugEnabled": false,
      "nativeDebugging": false
    }
  },
  "$schema": "https://json.schemastore.org/launchsettings.json"
}